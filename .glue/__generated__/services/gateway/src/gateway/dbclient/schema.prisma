generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String   @id @default(uuid()) @db.VarChar(36)
  name            String   @db.VarChar(255)
  content         String?
  githubId        String
  githubAvatarUrl String
  emailId         String?
  createdAt       DateTime @default(now())
}
